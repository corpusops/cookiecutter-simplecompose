{
    "_extensions": ["e.version_compare", "e.regex_replace"],
    "name": "myapp",
    "author": "foo",
    "email": "foo@org.com",
    "git_ns": "{{cookiecutter.name}}-group",
    "lname": "{{cookiecutter.name|lower|replace('docker-', '')|replace('-', '')}}",
    "eggname": "{{cookiecutter.lname}}",
    "infra_domain": "example.com",
    "simple_docker_image": "{{cookiecutter.git_ns.split('/')[0].lower()}}/{{cookiecutter.lname}}",
    "py_ver": "3.10",
    "network": "172.38.0.0",
    "with_sudo": "y",
    "main_branch": "main",
    "with_logging": "y",
    "restart_policy": "unless-stopped",
    "with_pyapp": "y",
    "with_mailcatcher": "y",
    "with_node": "y",
    "include_git_in_dockerimage": "y",
    "with_githubactions": "y",
    "node_version": "lts/*",
    "git_server": "gitlab.{{cookiecutter.infra_domain}}",
    "git_project_server": "{{cookiecutter.git_server}}",
    "git_project": "{{cookiecutter.lname}}",
    "git_scheme": "https",
    "git_user": "",
    "git_project_url": "{{cookiecutter.git_scheme}}://{%if cookiecutter.git_user%}{{cookiecutter.git_user}}@{%endif%}{{cookiecutter.git_project_server}}/{{cookiecutter.git_ns}}/{{cookiecutter.git_project}}",
    "out_dir": ".",
    "docker_registry": "",
    "app_type": "simplecompose",
    "ck_type": "{{cookiecutter.app_type}}",
    "docker_image": "{%if cookiecutter.docker_registry%}{{cookiecutter.docker_registry}}/{%endif%}{{cookiecutter.simple_docker_image}}",
    "db_image": "corpusops/postgres:13",
    "release_mode": "pypi|docker",
    "log_port": "10515",
    "corpusops_image": "corpusops/ubuntu-bare:22.04",
    "rsyslog_image": "corpusops/rsyslog",
    "base_image": "{{cookiecutter.corpusops_image}}"
}
